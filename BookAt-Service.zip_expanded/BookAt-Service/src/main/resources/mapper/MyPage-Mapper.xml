<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bookat.mapper.EventMapper">

	<select id="getReservations" resultType="Reservation">
		SELECT * FROM reservation WHERE user_id = #{userId}
	</select>
	
	<select id="getTickets" resultType="Ticket">
		SELECT * FROM ticket WHERE reservation_id = #{reservationId}
	</select>
	
</mapper>
